URxvt.perl-ext-common: font-size,keyboard-select
URxvt.scrollBar: false
URxvt.font: xft:Monospace:pixelsize=15:antialias=true:hinting=true:semibold
URxvt.boldFont: xft:Monospace:pixelsize=15:antialias=true:hinting=true:bold
URxvt.letterSpace: -1

!URxvt.transparent: true
!URxvt.shading: 20

URxvt.depth: 32
URxvt.background: rgba:1c00/1c00/1c00/F000
!00F6 is 'รถ'
URxvt.keysym.A-00F6: perl:keyboard-select:activate
URxvt.keyboard-select.clipboard: true
