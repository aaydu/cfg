!monokai
*background: #1c1c1c
*foreground: #d0d0d0
*cursorColor: #d0d0d0
*color0: #101010
*color1: #960050
*color2: #66aa11
*color3: #c47f2c
*color4: #30309b
*color5: #7e40a5
*color6: #3579a8
*color7: #9999aa
*color8: #303030
*color9: #ff0090
*color10: #80ff00
*color11: #ffba68
*color12: #5f5fee
*color13: #bb88dd
*color14: #4eb4fa
*color15: #d0d0d0

URxvt.perl-ext-common: font-size,keyboard-select
URxvt.scrollBar: false
URxvt.font: xft:Monospace:pixelsize=15:antialias=true:hinting=true:semibold
URxvt.boldFont: xft:Monospace:pixelsize=15:antialias=true:hinting=true:bold
URxvt.letterSpace: -1

!URxvt.transparent: true
!URxvt.shading: 20

URxvt.depth: 32
URxvt.background: rgba:1c00/1c00/1c00/F000
!00F6 is 'รถ'
URxvt.keysym.A-00F6: perl:keyboard-select:activate
URxvt.keyboard-select.clipboard: true

URxvt.geometry: 240x84
URxvt*.termName: screen-256color
